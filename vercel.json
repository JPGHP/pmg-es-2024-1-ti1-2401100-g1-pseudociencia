{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/public/mock_data/(.*)", "dest": "/public/mock_data/$1" },
      { "src": "/(.*)", "dest": "/public/$1" }
    ]
  }